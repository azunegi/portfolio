{
  "private": true,
  "name": "gatsby-starter-typescript-eslint",
  "version": "0.1.3",
  "description": "Simple gatsby starter that can use TypeScript and ESLint",
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lintfix": "eslint . --fix --ext .js,.ts,.tsx",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tetsugi/gatsby-starter-typescript-eslint"
  },
  "keywords": [
    "gatsby",
    "typescript",
    "starter"
  ],
  "author": "tetsugi",
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "gatsby": "^2.20.9",
    "gatsby-plugin-emotion": "^4.2.1",
    "gatsby-plugin-graphql-codegen": "^2.7.0",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sass": "^2.3.4",
    "gatsby-plugin-smoothscroll": "^1.2.0",
    "gatsby-plugin-transition-link": "^1.20.2",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-plugin-typography": "^2.5.10",
    "gatsby-remark-autolink-headers": "^2.3.13",
    "gsap": "^3.5.1",
    "modern-css-reset": "^1.1.0",
    "node-sass": "^4.14.1",
    "particles-bg": "^2.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^6.8.3",
    "react-modal": "^3.11.2",
    "react-scroll": "^1.8.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-github": "^0.16.19"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/node": "^13.9.8",
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "gh-pages": "^3.1.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
